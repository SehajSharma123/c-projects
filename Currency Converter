#include <iostream>
#include <map>
using namespace std;

int main() {
    map<string, double> rates = {{"USD", 1.0}, {"EUR", 0.92}, {"INR", 83.0}, {"GBP", 0.79}};
    double amount; string from, to;
    
    cout << "Enter amount: "; cin >> amount;
    cout << "From (USD, EUR, INR, GBP): "; cin >> from;
    cout << "To (USD, EUR, INR, GBP): "; cin >> to;

    double result = amount * (rates[to] / rates[from]);
    cout << amount << " " << from << " is " << result << " " << to << endl;
    return 0;
}
